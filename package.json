{
  "name": "git-switch",
  "version": "1.5.1",
  "description": "Cross-platform electron app for managing git users while pair/mob programming",
  "main": "dist/main.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:compile",
    "build:clean": "rimraf ./dist",
    "build:compile": "webpack",
    "lint": "yarn lint:main && yarn lint:renderer && yarn lint:cli",
    "lint:cli": "eslint ./src/cli",
    "lint:main": "eslint ./src/common",
    "lint:renderer": "eslint ./src/client",
    "make:deb": "electron-forge make --arch=x64 --platform=linux --targets=@electron-forge/maker-deb",
    "make:rpm": "electron-forge make --arch=x64 --platform=linux --targets=@electron-forge/maker-rpm",
    "make:macos": "electron-forge make --arch=x64 --platform=darwin",
    "make:windows": "electron-forge make --arch=x64 --platform=win32",
    "pack": "yarn pack:macos && yarn pack:linux && yarn pack:windows",
    "pack:linux": "electron-forge package --arch=x64 --platform=linux",
    "pack:macos": "electron-forge package --arch=x64 --platform=darwin",
    "pack:windows": "electron-forge package --arch=x64 --platform=win32",
    "packages:zip": "node ./scripts/package-zipper",
    "release:clean": "rimraf ./out",
    "release:create": "yarn release:clean && yarn build && yarn pack && yarn packages:zip",
    "release:publish": "node ./scripts/release-publisher",
    "start": "electron-forge start",
    "start:macos": "yarn build && yarn pack:macos && open out/git-switch-darwin-x64/git-switch.app",
    "start:dev": "cross-env NODE_ENV=development yarn build && yarn start",
    "test": "mocha",
    "test:ci": "mocha --reporter tap",
    "test:watch": "yarn test -- --watch"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/git-switch-electron"
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/register": "7.9.0",
    "@electron-forge/cli": "6.0.0-beta.51",
    "@electron-forge/maker-deb": "6.0.0-beta.51",
    "@electron-forge/maker-rpm": "6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "6.0.0-beta.51",
    "@electron-forge/maker-zip": "6.0.0-beta.51",
    "archiver": "4.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "electron": "8.2.5",
    "eslint": "7.0.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-jsx": "8.1.0",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-mocha": "7.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-standard": "4.0.1",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lodash.capitalize": "4.2.1",
    "lodash.orderby": "4.6.0",
    "md5": "2.2.1",
    "menubar": "8.0.2",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "7.1.2",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "svg-react-loader": "0.4.6",
    "uuid": "8.0.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "yargs": "15.3.1"
  },
  "resolutions": {
    "kind-of": "^6.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test && yarn build"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "com.pluralsight.git-switch",
        "icon": "./src/assets/icon",
        "ignore": [
          "^/\\..+",
          "^/(scripts|src|test)",
          "(babel|webpack).config.js",
          "yarn.lock",
          "\\.sketch$"
        ],
        "overwrite": true,
        "quiet": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "git_switch_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "github_repository": {
        "owner": "pluralsight",
        "name": "git-switch-electron"
      }
    }
  }
}
